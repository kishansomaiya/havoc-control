{
  "name": "jupiter-csg-frontend-redux-playwright-ui-tests",
  "version": "1.0.0",
  "description": "UI Test Automation Framework using Playwright",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "pnpm exec playwright test",
    "test:debug": "pnpm exec playwright test --debug",
    "test:critical": "pnpm exec playwright test --project critical_paths",
    "test:ui": "pnpm exec playwright test --ui",
    "test:dev:ui": "UI_TEST_ENV=dev pnpm exec playwright test --ui",
    "test:stg:ui": "UI_TEST_ENV=stg pnpm exec playwright test --ui",
    "test:prod:ui": "UI_TEST_ENV=prod pnpm exec playwright test --ui",
    "test:headed": "pnpm exec playwright test --headed",
    "test:dev:headed": "UI_TEST_ENV=dev pnpm exec playwright test --headed",
    "test:stg:headed": "UI_TEST_ENV=stg pnpm exec playwright test --headed",
    "test:report": "pnpm exec playwright show-report",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test:proj:resultset": "pnpm exec playwright test --project='chromium auth_resultset' && pnpm exec playwright test --project='cleanup' --ignore-dependencies",
    "test:proj:authenticated": "pnpm exec playwright test --project='chromium authenticated'",
    "test:proj:non_authenticated": "pnpm exec playwright test --project='chromium non-authenticated'"
  },
  "keywords": [
    "playwright",
    "ui",
    "test",
    "automation"
  ],
  "author": "iaroslav proskurnia",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@playwright/test": "^1.49.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "axios": "^1.7.2",
    "dd-trace": "^5.67.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.8.0",
    "mammoth": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "typescript-eslint": "^7.16.0"
  },
  "dependencies": {
    "csv-parser": "^3.0.0"
  }
}
